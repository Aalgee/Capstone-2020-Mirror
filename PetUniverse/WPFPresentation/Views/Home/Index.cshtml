@{
    ViewBag.Title = "Home Page";
}

<span></span>
@Html.Partial("_CarouselPartial")

<section>
    <div class="productInfo">
        &nbsp;
        <div class="row justify-content-center">
            <div class="col-md-8">
                <div class="row mb-4">
                    <a href="https://unsplash.it/1200/768.jpg?image=251" data-toggle="lightbox" data-gallery="example-gallery" class="col-sm-4">
                        <img src="https://unsplash.it/600.jpg?image=251" class="img-fluid">
                    </a>
                    <a href="https://unsplash.it/1200/768.jpg?image=252" data-toggle="lightbox" data-gallery="example-gallery" class="col-sm-4">
                        <img src="https://unsplash.it/600.jpg?image=252" class="img-fluid">
                    </a>
                    <a href="https://unsplash.it/1200/768.jpg?image=253" data-toggle="lightbox" data-gallery="example-gallery" class="col-sm-4">
                        <img src="https://unsplash.it/600.jpg?image=253" class="img-fluid">
                    </a>
                </div>

            </div>
        </div>
    </div>
</section>


<div>
    <h2 class=" text-center text-uppercase">Popular Organic Brands</h2>
    <h3 class="  text-bold text-center">Buy one, get one save 15%</h3>
</div>

<div>
    &nbsp;
    <ul class="list-unstyled list-inline row padding-top-lg">
        <li class="col-2 rd-brand-tile">
            <a href="!#" onclick="">
                <div>
                    <img src="~/Content/Images/BlueBuffaloBasicsLimitedIngredientDietGrainFreeNaturalIndoorAdultDryCatFood Resized.jpg" lazyimg="loaded" data-src="//assets.petco.com/petco/image/upload/f_auto,q_auto/popular-rd-brands-031320-wholehearted-170w-170h-d" alt="WholeHearted" class="center-block img-responsive" style="width:auto;">
                </div>
            </a>
        </li>

        <li class="col-2 rd-brand-tile">
            <a href="!#" onclick="clickTrackLink(this);">
                <div>
                    <img src="~/Content/Images/OrganixGrainFreeCaston&Pollux resized.jpg" lazyimg="loaded" data-src="//assets.petco.com/petco/image/upload/f_auto,q_auto/popular-rd-brands-031320-greenies-170w-170h-d" alt="Greenies" class="center-block img-responsive" style="width:auto;">
                </div>
            </a>
        </li>

        <li class="col-2 rd-brand-tile">
            <a href="!#" onclick="">
                <div>
                    <img src="~/Content/Images/OrganixDogFood resized.jpg" lazyimg="loaded" data-src="//assets.petco.com/petco/image/upload/f_auto,q_auto/popular-rd-brands-031320-taste-of-the-wild-170w-170h-d" alt="Taste of the Wild" class="center-block img-responsive" style="width:auto;">
                </div>
            </a>
        </li>

        <li class="col-2 rd-brand-tile">
            <a href="!#" onclick="">
                <div>
                    <img src="~/Content/Images/RealMeatOrganicDogFoodTruDog resized.jpg" alt="Merrick" class="center-block img-responsive" style="width:auto;">
                </div>
            </a>
        </li>

        <li class="col-2 rd-brand-tile">
            <a href="!#" onclick="">
                <div>
                    <img src="~/Content/Images/BudgetPickZeroGrainGrainFreeCatsRachelRay resizeed.jpg" lazyimg="loaded" data-src="//assets.petco.com/petco/image/upload/f_auto,q_auto/popular-rd-brands-031320-blue-buffalo-170w-170h-d" alt="Blue Buffalo" class="center-block img-responsive" style="width:auto;">
                </div>
            </a>
        </li>

        <li class="col-2 rd-brand-tile">
            <a href="!#" onclick="">
                <div>
                    <img src="~/Content/Images/NewmansOwnPremiumCannedFormulasForCats3oz resized.jpg" lazyimg="loaded" data-src="//assets.petco.com/petco/image/upload/f_auto,q_auto/popular-rd-brands-031320-royal-canin-170w-170h-d" alt="Royal Canin" class="center-block img-responsive" style="width:auto;">
                </div>
            </a>
        </li>
    </ul>
</div>
<div class="text-center padding-top-lg">
    <a href="#!" class="btn btn-primary" onclick="clickTrackLink(this);">Start Saving</a>
</div>





<div class="row">
    <div class="col-md-7">
        <a href="#">
            <img class="img-fluid rounded mb-3 mb-md-0" src="~/Content/Images/happyManDogWalker.jpeg" />
        </a>
    </div>
    <div class="col-md-5">
        <h3>A few words from our participants...</h3>
        <blockquote class="blockquote text-center align-content-center ">
            <div class="bottomdiv">
                <p class="mb-0">
                    I volunteered at Pet Universe and I can honestly say I haven't met a more dedicated and kind group of people.
                    I enjoyed every job that I tried, but dog walking was my favorite!
                </p>
                <footer class="blockquote-footer">Ragnar White in <cite title="Source Title">Pet Universe Weekly</cite></footer>
            </div>

        </blockquote>
    </div>
</div>

<hr>

<div class="row">
    <div class="col-md-7">
        <a href="#">
            <img class="img-fluid rounded mb-3 mb-md-0" src="~/Content/Images/happyWoman.jpeg" />
        </a>
    </div>
    <div class="col-md-5">
        <blockquote class="blockquote text-center align-content-center">
            <div class="bottomdiv">
                <p class="mb-0">
                    Everyone, employees and volunteers alike were so helpful through the adoption process, and were SO accomodating
                    when I had questions, and concerns. I will be forever grateful..they helped me find the perfect pet, and my best friend!
                </p>
                <footer class="blockquote-footer">Natalie Cobbler in<cite title="Source Title">Pet Universe Weekly</cite></footer>
            </div>
        </blockquote>
    </div>
</div>

<hr>


